generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Strain {
  id               Int      @id @default(autoincrement())
  name             String
  cannabinoids     Json
  terpenes         Json
  compositeProfile Json?
  effects          String?
  lineage          String?
  family           String?
  tags             String[]
  createdAt        DateTime @default(now())
}
